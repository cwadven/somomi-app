{
  "name": "somomi",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "build-android": "npm run bundle-android && cd android && ./gradlew assembleRelease -x lint && cd ..",
    "update:dev": "eas update --branch development --message \"개발 환경 업데이트\"",
    "update:prod": "eas update --branch production --message \"프로덕션 환경 업데이트\""
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "~1.21.0",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.6.2",
    "expo": "^50.0.0",
    "expo-auth-session": "~5.2.1",
    "expo-constants": "~15.4.5",
    "expo-image-picker": "~14.7.1",
    "expo-notifications": "~0.27.6",
    "expo-secure-store": "~12.8.1",
    "expo-status-bar": "~1.11.1",
    "expo-task-manager": "^13.1.6",
    "expo-updates": "~0.24.13",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.0.4",
    "expo-modules-autolinking": "~1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@react-native-community/cli": "^12.3.0"
  },
  "private": true
}
